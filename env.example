# Backend core
DATABASE_URL=postgresql://user:pass@host:5432/db
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_JWT_SECRET=your-jwt-secret

# Upstash Redis (REST)
UPSTASH_REDIS_REST_URL=https://your-upstash-endpoint.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-upstash-token

# Optional Redis URL (for redis:// clients)
REDIS_URL=redis://:password@host:6379/0

# Vertex AI / Gemini
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=europe-west1
GOOGLE_APPLICATION_CREDENTIALS=/path/to/gcp-service-account.json
VERTEX_AI_USE_ADC=true
VERTEX_AI_API_KEY=
INFERENCE_SIMPLE=

# API + internal auth
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_BACKEND_URL=http://localhost:8000
RF_INTERNAL_KEY=your-internal-key
DEFAULT_TENANT_ID=00000000-0000-0000-0000-000000000000

# Frontend Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Frontend inference config (optional)
NEXT_PUBLIC_GCP_PROJECT_ID=your-gcp-project-id
NEXT_PUBLIC_GCP_REGION=europe-west1
NEXT_PUBLIC_INFERENCE_SIMPLE=
NEXT_PUBLIC_VERTEX_AI_API_KEY=
NEXT_PUBLIC_MODEL_GENERAL=gemini-2.5-flash-lite
