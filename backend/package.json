{
  "name": "raptorflow-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:orchestrator": "tsx watch src/orchestrator/server.ts",
    "build": "tsc",
    "build:orchestrator": "tsc --project tsconfig.orchestrator.json",
    "start": "node dist/index.js",
    "start:orchestrator": "node dist/orchestrator/server.js",
    "test": "vitest",
    "test:ci": "vitest run --coverage --reporter=json",
    "test:orchestrator": "vitest src/orchestrator/ src/v2/agents/ src/v2/llm/ src/services/",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "demo:agentic": "tsx src/v2/demo_advanced_agentic.ts",
    "orchestrator:agents": "tsx src/orchestrator/scripts/generate-agent-manifests.ts",
    "orchestrator:worker": "tsx src/orchestrator/worker.ts"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.17.0",
    "@google-cloud/vertexai": "^1.9.2",
    "@langchain/core": "^0.3.0",
    "@langchain/google-vertexai": "^0.1.0",
    "@langchain/langgraph": "^0.2.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.2",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@supabase/supabase-js": "^2.45.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@upstash/redis": "^1.34.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "cors": "^2.8.5",
    "puppeteer": "^22.6.0",
    "handlebars": "^4.7.8",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.3",
    "langchain": "^0.3.0",
    "prom-client": "^15.1.3",
    "uuid": "^13.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.9.0",
    "prettier": "^3.3.0",
    "supertest": "^7.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vitest": "^4.0.16"
  }
}
