# Raptorflow Environment Variables
# Copy this file to .env.local and fill in your actual API keys

# Google Maps API Key
# Get your key from: https://console.cloud.google.com/google/maps-apis
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Vertex AI Configuration
# Get your API key from: https://console.cloud.google.com/apis/credentials
VITE_VERTEX_AI_API_KEY=your_vertex_ai_api_key_here
# Vertex AI regional endpoint (e.g., us-central1-aiplatform.googleapis.com)
VITE_VERTEX_AI_ENDPOINT=us-central1-aiplatform.googleapis.com
# Vertex AI model ID (e.g., gemini-2.5-flash, gemini-2.0-flash-exp)
VITE_VERTEX_AI_MODEL=gemini-2.5-flash

# IMPORTANT: Never commit your actual .env.local file to version control
# Add .env.local to your .gitignore file
# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google Maps API (for location features)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# PostHog Analytics
VITE_POSTHOG_KEY=your_posthog_project_key
VITE_POSTHOG_HOST=https://app.posthog.com

# Environment
VITE_ENVIRONMENT=development
